{
  "name": "chat-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.4.0",
    "browserify-zlib": "^0.2.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "next": "^13.4.4",
    "pm2": "^5.3.0",
    "querystring-es3": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "react-scripts": "5.0.1",
    "react-use-websocket": "^4.3.1",
    "sass": "^1.62.1",
    "stream-http": "^3.2.0",
    "web-vitals": "^2.1.4",
    "ws": "^8.13.0"
  },
  "scripts": {
    "start": "concurrently \"npm run start-react\" \"npm run server-start\" \"npm run logs\"",
    "start2": "set PORT=2999 && react-scripts start",
    "start-react": "react-scripts start",
    "server-start": "npm run server-start-auth-port-server && npm run server-start-friend-requests-server && npm run server-start-ws && npm run server-start-friend-chat",
    "server-stop": "npm run server-stop-auth-port-server && npm run server-stop-friend-requests-server && npm run server-restart-ws && npm run server-stop-friend-chat",
    "server-restart": "npm run server-restart-auth-port-server && npm run server-restart-friend-requests-server && npm run server-stop-ws && npm run server-restart-friend-chat",

    "server-start-auth-port-server": "pm2 start src/api/auth.js --name auth-server",
    "server-start-friend-requests-server": "pm2 start src/api/friendRequest.js --name friend-requests-server",
    "server-start-ws": "pm2 start src/api/ws-connection.js --name ws-connection",
    "server-start-friend-chat": "pm2 start src/api/friendsChat.js --name friendsChat",

    "server-stop-auth-port-server": "pm2 stop auth-server",
    "server-stop-friend-requests-server": "pm2 stop friend-requests-server",
    "server-stop-ws": "pm2 stop ws-connection",
    "server-stop-friend-chat": "pm2 stop friendsChat",

    "server-restart-auth-port-server": "pm2 restart auth-server",
    "server-restart-friend-requests-server": "pm2 restart friend-requests-server",
    "server-restart-ws": "pm2 restart ws-connection",
    "server-restart-friend-chat": "pm2 restart friendsChat",

    "logs": "pm2 logs",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^8.0.1"
  }
}
